{
    "container": {
        "position": "fixed",
        "flex-direction": "column",
        "height": "100vh",
        "width": "92vw",
        "right": "0"
    },
    "style1": {
        "bgColor": "var(--primarydarkblue)",
        "flexDirection": "column",
        "height": "100vh",
        "width": "100%"
    },
    "style2": {
        "border": "none",
        "padding": "0.75rem 1rem",
        "borderRadius": "50%",
        "fontSize": "14px"
    },
    "style3": {
        "justifyContent": "center",
        "alignItems": "center",
        "width": "100%"
    },
    "style4": {
        "display": "flex",
        "justifyContent": "space-between",
        "verticalAlign": "center",
        "width": "100%"
    },
    "style5": {
        "textOverflow": "ellipsis",
        "maxWidth": "45vw",
        "overflow": "hidden",
        "whiteSpace": "nowrap",
        "textAlign": "left",
        "marginBottom": "auto",
        "marginTop": "auto"
    },
    "top_section": {
        "width": "100%",
        "height": "80px",
        "font-family": "'Mulish', sans-serif",
        "font-weight": "700",
        "font-size": "1.4rem",
        "display": "flex",
        "flex-wrap": "warp",
        "align-item": "center",
        "color": "white",
        "padding": "0px 3.5vw",
        "background-color": "var(--primarygreen)"
    },
    "top_section button": {
        "background-color": "white",
        "color": "var(--primarygreen)",
        "font-size": "20px"
    },
    "headingContainer": {
         "width": "100%",
         "display": "flex",
         "justify-content": "space-between"
    },
    "avatarContainer": {
        "display": "felx",
        "align-items": "center",
        "height": "67px",
        "width": "100%"
    },
    "innerRing": {
        "width": "70px",
        "height": "55px",
        "border-radius": "50%",
        "border": "3px solid green",
        "margin-right": "10px"
    },
    "toggleButtonConatainer": {
        "display": "felx",
        "justify-content": "center",
        "align-items": "flex-start",
        "margin-right": "1rem"
    },
    "userName": {
        "display": "felx",
         "align-items": "center",
         "padding-left": "0.5rem",
         "height": "80px"
    },
    "chatWindow": {
        "height": "100vh",
        "width": "100%",
        "margin": "auto",
        "display": "flex",
        "justify-content": "center",
        "align-items": "flex-start",
        "background-color": "var(--primaryyellow)"
    },
    "BackBox": {
        "width": "100vw",
        "height": "90vh",
        "border-radius": "25px 25px 0px 0px",
        "display": "flex",
        "flex-direction": "column",
        "justify-content": "flex-start"
    },
    "wallpaper1": {
        "width": "82vw",
        "height": "85vh",
        "margin": "auto",
        "border-radius": "14px",
        "display": "flex",
        "flex-direction": "column",
        "justify-content": "flex-start",
        "background-image": "url()"
    },
    "wallpaper": {
        "width": "82vw",
        "height": "85vh",
        "margin": "auto",
        "border-radius": "14px",
        "display": "flex",
        "flex-direction": "column",
        "justify-content": "flex-start",
        "background-image": "url()"
    }
}