{
    "container": {
    "position": "fixed",
    "flex-direction": "column",
    "height": "100vh",
    "width": "92vw",
    "right": "0"
    },
    "top_section": {
    "width": "100%",
    "height": "80px",
    "font-family": "'Mulish', sans-serif",
    "font-weight": "700",
    "font-size": "1.4rem",
    "display": "flex",
    "flex-wrap": "wrap",
    "align-items": "center",
    "justify-content": "space-between",
    "color": "white",
    "padding": "0px 3.5vw",
    "background-color": "var(--primarygreen)"
    },
    "top_section button": {
    "background-color": "white",
    "color": "var(--primarygreen)",
    "font-size": "20px"
    },
    "headingContainer": {
    "width": "100%",
    "display": "flex",
    "justify-content": "space-between"
    },
    "avatarContainer": {
    "display": "flex",
    "align-items": "center",
    "height": "67px"
    },
    "innerRing": {
    "width": "60px",
    "height": "55px",
    "border-radius": "50%",
    "border": "3px solid green",
    "margin-right": "10px"
    },
    "toggleButtonContainer": {
    "display": "flex",
    "justify-content": "center",
    "align-items": "flex-start",
    "margin-right": "1rem"
    },
    "UserName": {
    "display": "flex",
    "align-items": "center",
    "padding-left": "0.5rem",
    "height": "80px"
    },
    "chatWindow": {
    "height": "100vh",
    "width": "100%",
    "margin": "auto",
    "display": "flex",
    "justify-content": "center",
    "align-items": "flex-start",
    "background-color": "var(--primaryyellow)"
    },
    "BackBox": {
    "width": "100vw",
    "height": "90vh",
    "border-radius": "25px 25px 0px 0px",
    "display": "flex",
    "flex-direction": "column",
    "justify-content": "flex-start"
    },
    "wallpaper1": {
    "width": "82vw",
    "height": "85vh",
    "margin": "auto",
    "border-radius": "14px",
    "display": "flex",
    "flex-direction": "column",
    "justify-content": "flex-start",
    "background-image": "url('https://wallpaperaccess.com/full/51364.jpg')"
    },
    "wallpaper2": {
    "width": "82vw",
    "height": "85vh",
    "margin": "auto",
    "border-radius": "14px",
    "display": "flex",
    "flex-direction": "column",
    "justify-content": "flex-start",
    "background-image": "url('https://cdn.pixabay.com/photo/2017/08/30/01/05/milky-way-2695569__480.jpg')"
    }
}