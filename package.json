{
  "name": "uci",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/uci/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "cross-env": "^7.0.3",
    "socket-package": "^0.4.0"
  }
}
