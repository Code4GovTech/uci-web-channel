{
  "name": "uci",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/uci/*",
    "packages/*",
    "tsdx/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@types/redux-logger": "^3.0.9",
    "eslint": "^8.47.0",
    "eslint-config-custom": "^0.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{md,ts,tsx}": [
      "prettier --check"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "react-redux": "^8.1.2",
    "redux-logger": "^3.0.6",
    "socket-package": "^0.4.0",
    "styled-components": "^6.0.7"
  }
}
